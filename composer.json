    "keywords": [
        "appstract",
        "refererredirector",
        "referer-redirector"
    ],
    "homepage": "https://github.com/appstract/laravel-referer-redirect",
    "license": "MIT",
    "authors": [
        {
            "name": "Teun de Kleijne",
            "email": "hello@appstract.team",
            "homepage": "https://appstract.team",
            "role": "Developer"
        }
    ],
    "require": {
